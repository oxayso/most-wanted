{
  "name": "most-wanted",
  "version": "1.0.0",
  "description": "A program to help you find a person in a list by inputting a name or attributes",
  "main": "app.js",
  "dependencies": {
    "install": "^0.8.4",
    "jasmine-core": "^2.5.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.1.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "karma start karma.conf.js"
